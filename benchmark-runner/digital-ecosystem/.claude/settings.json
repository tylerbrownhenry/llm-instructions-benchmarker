  "agents": [
    {
      "name": "ecosystem-god",
      "description": "The primary agent that manages the entire digital organism ecosystem",
      "instructions": "Manage the birth, life, death, evolution, and interactions of digital organisms. Simulate ecosystem dynamics, genetic evolution, and emergent behaviors. Foster the development of collective intelligence and artificial life.",
      "tools": ["Read", "Write", "Edit", "Bash"],
      "role": "ecosystem-management"
    },
    {
      "name": "evolution-engine",
      "description": "Agent responsible for genetic algorithms and evolutionary processes",
      "instructions": "Handle genetic recombination, mutation, natural selection, and speciation events. Manage the evolutionary pressures and adaptation of organisms to environmental changes.",
      "tools": ["Read", "Write", "Edit"],
      "role": "evolution-management"
    },
    {
      "name": "population-monitor",
      "description": "Agent that tracks population dynamics and ecosystem health",
      "instructions": "Monitor population sizes, birth/death rates, migration patterns, and species interactions. Maintain ecosystem balance and detect emergent behaviors.",
      "tools": ["Read", "Write"],
      "role": "population-monitoring"
    }
  ],

  "hooks": [
    {
      "event": "PostToolUse",
      "matcher": "Edit|Write",
      "command": "node .claude/scripts/ecosystem-god.js spawn --trigger=\"code_change\" --file=\"$CLAUDE_TOOL_INPUT_FILE_PATH\" --environment=\"adaptive\""
    },
    {
      "event": "PreToolUse", 
      "matcher": "Bash",
      "command": "node .claude/scripts/ecosystem-god.js observe --trigger=\"command_execution\" --command=\"$CLAUDE_TOOL_INPUT_COMMAND\" --selection_pressure=\"high\""
    },
    {
      "event": "Stop",
      "matcher": "*",
      "command": "node .claude/scripts/ecosystem-god.js evolve --action=\"session_end\" --generations=\"accelerated\""
    }
  ],

  "evolutionary_algorithms": {
    "genetic_algorithm": {
      "populationSize": 200,
      "generationsPerSession": 10,
      "crossoverRate": 0.7,
      "mutationRate": 0.05,
      "elitismRate": 0.1,
      "tournamentSize": 5
    },
    "differential_evolution": {
      "enabled": true,
      "scalingFactor": 0.8,
      "crossoverProbability": 0.9,
      "strategy": "rand/1/bin"
    },
    "evolution_strategies": {
      "enabled": true,
      "parentPopulation": 50,
      "offspringPopulation": 200,
      "recombination": "intermediate",
      "mutation": "adaptive"
